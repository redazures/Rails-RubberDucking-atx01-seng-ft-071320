<%if flash[:my_errors]%>
    <%flash[:my_errors].each do |error|%>
    <p><%=error%></p>
    <%end%>
<%end%>
<%= form_for @student do |f| %>
    <p>
    <%=f.label :name %>
    <%=f.text_field :name %>
    </p>
    <p>
    <%=f.label :mod %>
    <%=f.number_field :mod %>
    </p>
    <p>
    <%=f.submit%>
    </p>
<% end %>